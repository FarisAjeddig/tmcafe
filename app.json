{
  "expo": {
    "name": "Thema Cafe Montpellier",
    "slug": "TCM",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#1a2357"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "./assets/images/*",
      "./constants/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ThemaCafeMontpellier.TCM"
    }
  }
}
